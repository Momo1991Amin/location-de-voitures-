{% extends 'base.html.twig' %}

{% block title %}Voici nos différents véhicules par catégory{% endblock %}

{% block body %}   
<div class="container">
    <h1 class="my-5">Liste des véhicules par catégories !</h1>
    <section class="row" id="cars"  >
        {% for car in cars %}
            {% include "partials/_product.html.twig" with {'car' : car} %}
        {% endfor %}
    </section>
    <div>
        {{ knp_pagination_render(cars) }}
    </div>
</div>    
{% endblock %}